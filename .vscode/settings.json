{
  "eslint.enable": true, //是否开启vscode的eslint
  "eslint.validate": ["javascript", "typescript"],
  "eslint.options": {
    //指定vscode的eslint所处理的文件的后缀
    "extensions": [
      ".js",
      ".ts",
    ]
  },
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "editor.tabSize": 2
}
